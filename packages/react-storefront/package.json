    "react-svg-loader": "^2.1.0",
    "react-transition-group": "^2.5.0",
    "react-universal-component": "^2.9.0",
    "webpack": "^3.11.0",
    "webpack-flush-chunks": "^1.2.3"
  },
  "devDependencies": {
    "@storybook/react": "^3.4.8",
    "babel-cli": "^6.26.0",
    "babel-core": "^6.26.3",
    "babel-jest": "^22.4.3",
    "babel-loader": "^7.1.4",
    "babel-plugin-module-resolver": "^3.1.1",
    "babel-plugin-react-storefront": "^5.3.0",
