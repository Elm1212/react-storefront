{
  "name": "react-storefront",
  "version": "5.5.0",
  "description": "Build and deploy e-commerce progressive web apps in record time.",
  "browser": "src",
  "module": "./index.js",
  "author": "Moovweb",
  "license": "UNLICENSED",
  "scripts": {
    "test:unit": "jest",
    "test": "jest test.js && yarn test:browser",
    "test:browser": "webpack --config=config/webpack.config.js && karma start config/karma.conf.js --single-run",
    "storybook": "start-storybook -p 6006",
    "build-storybook": "build-storybook",
    "deploy-storybook": "gh-pages -d storybook-static",
    "create-indexes": "create-index -ri --banner \"/**\n * @license\n * Copyright Â© 2017-2018 Moov Corporation.  All rights reserved.\n */\" src",
    "watch": "yarn build && babel --watch src --watch webpack --source-maps --out-dir lib",
    "clean": "mkdir -p lib; find lib -not -name 'node_modules' -not -name 'package-lock.json' -delete",
    "build": "yarn run clean && yarn create-indexes && NODE_ENV=production babel $@ src --source-maps --out-dir lib && cp package.json lib && cp -r webpack docs lib && cp -r service-worker lib",
    "docs": "yarn build-storybook && yarn deploy-storybook && mkdir -p docs/build && babel-node docs/buildComponentDocs.js"
  },
  "peerDependencies": {
    "@material-ui/core": "^3.0.0",
    "@material-ui/icons": "^3.0.0",
    "babel-plugin-react-storefront": "^5.0.0",
    "eslint-config-react-app": "^2.1.0",
    "file-loader": "^1.1.11",
    "mobx": "^4.1.0",
    "mobx-react": "^5.0.0",
    "mobx-state-tree": "^2.0.1",
